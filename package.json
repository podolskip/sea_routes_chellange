{
  "name": "interview-excersise",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.9.3",
    "@material-ui/icons": "4.9.1",
    "@react-google-maps/api": "^1.12.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.64",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/request": "^2.48.5",
    "csvtojson": "^2.0.10",
    "highcharts": "^8.2.0",
    "highcharts-react-official": "^3.0.0",
    "jest": "24.9.0",
    "lodash.clonedeep": "^4.5.0",
    "react": "^16.13.1",
    "react-async-script-loader": "^0.3.0",
    "react-dependent-script": "^1.1.2",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.4.3",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0",
    "request": "^2.88.2",
    "rxjs": "^6.2.2",
    "typescript": "^3.7.5",
    "typestyle": "^2.0.4"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/lodash.clonedeep": "^4.5.4",
    "@types/react-loadable": "^5.4.1",
    "@types/react-redux": "^5.0.21",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-router-redux": "^5.0.13",
    "@types/react-test-renderer": "^16.8.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "husky": "^1.1.0",
    "react-docgen-typescript": "^1.10.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^7.0.1",
    "ts-jest": "^26.4.1",
    "prettier": "^1.14.3",
    "tslint-config-prettier": "^1.18.0",
    "webpack-blocks": "2.0.1"
  },
  "scripts": {
    "addCommit": "npm run prettier:write && git add --all && git commit -m",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prettier:write": "npm run prettier:base -- --write \"{__tests__,src}/**/*.{ts,tsx}\"",
    "prettier:base": "prettier --parser typescript --single-quote"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:write",
      "pre-push": "jest",
      "post-merge": "npm install"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup/setup-enzyme.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src",
      "<rootDir>/__tests__",
      "src",
      "__tests__"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@material-ui.*?\\.js$))"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx|js)$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  }
}
